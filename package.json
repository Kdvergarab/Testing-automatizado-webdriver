{
  "name": "pruebaqa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "wdio run ./wdio.conf.js",
    "test:latam": "wdio run ./wdio.conf.js --spec ./features/latam_cotizacion.feature",
    "test:login": "wdio run ./wdio.conf.js --spec ./features/login.feature",
    "test:parallel": "wdio run ./wdio.conf.js --maxInstances 5",
    "test:all": "npm run test:latam && npm run test:login",
    "test:retry": "wdio run ./wdio.conf.js --specFileRetries 2",
    "test:debug": "wdio run ./wdio.conf.js --logLevel debug",
    "test:slow": "WDIO_TIMEOUT=30000 wdio run ./wdio.conf.js",
    "test:fast": "WDIO_TIMEOUT=5000 wdio run ./wdio.conf.js",
    "test:chrome": "wdio run ./wdio.conf.js",
    "test:firefox": "BROWSER=firefox wdio run ./wdio.conf.js",
    "test:safari": "BROWSER=safari wdio run ./wdio.conf.js",
    "test:dev": "BASE_URL=https://dev-example.com wdio run ./wdio.conf.js",
    "test:stage": "BASE_URL=https://stage-example.com wdio run ./wdio.conf.js",
    "test:prod": "BASE_URL=https://www.example.com wdio run ./wdio.conf.js",
    "test:ci": "REPORTER=junit wdio run ./wdio.conf.js --headless",
    "wdio": "wdio run ./wdio.conf.js",
    "wdio:headless": "wdio run ./wdio.conf.js --headless",
    "report": "allure generate --clean allure-results && allure open",
    "report:junit": "cat ./junit-results/*.xml",
    "clean:reports": "rm -rf allure-results allure-report junit-results"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^12.1.0",
    "@wdio/allure-reporter": "^9.18.0",
    "@wdio/browser-runner": "^9.18.4",
    "@wdio/browserstack-service": "^9.18.4",
    "@wdio/cli": "^9.18.4",
    "@wdio/cucumber-framework": "^9.18.0",
    "@wdio/local-runner": "^9.18.4",
    "@wdio/spec-reporter": "^9.18.0",
    "@wdio/visual-service": "^9.0.0",
    "allure-commandline": "^2.34.1",
    "chai": "^5.2.1",
    "chromedriver": "^138.0.4",
    "dotenv": "^17.2.1",
    "wdio-chromedriver-service": "^7.3.2"
  }
}
